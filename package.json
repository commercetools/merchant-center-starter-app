{
  "name": "merchant-center-starter-app",
  "version": "1.0.0",
  "description": "A starter example for building a Merchant Center application",
  "main": "index.js",
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "build": "mc-scripts build",
    "start": "mc-scripts start",
    "start:prod": "mc-http-server --use-local-assets",
    "now-start": "npm run start:prod",
    "lint": "jest --projects jest.eslint.config.js",
    "format": "npm run format:js && npm run format:md",
    "format:js": "prettier --write 'src/**/*.js'",
    "format:md": "prettier --parser markdown --write '*.md,src/**/*.md'",
    "test": "jest --config jest.test.config.js",
    "test:watch": "jest --config jest.test.config.js --watch",
    "ci": "jest --projects jest.*.config.js"
  },
  "dependencies": {
    "@commercetools-frontend/actions-global": "1.0.0-beta.11",
    "@commercetools-frontend/application-shell": "1.0.0-beta.11",
    "@commercetools-frontend/constants": "1.0.0-beta.11",
    "@commercetools-frontend/i18n": "1.0.0-beta.11",
    "@commercetools-frontend/l10n": "1.0.0-beta.11",
    "@commercetools-frontend/permissions": "1.0.0-beta.11",
    "@commercetools-frontend/sdk": "1.0.0-beta.11",
    "@commercetools-frontend/ui-kit": "1.0.0-beta.11",
    "prop-types": "15.6.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-loadable": "5.4.0",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "devDependencies": {
    "@commercetools-frontend/mc-scripts": "1.0.0-beta.11",
    "@commercetools-frontend/mc-http-server": "1.0.0-beta.11",
    "@commercetools-frontend/eslint-config-mc-app": "1.0.0-beta.11",
    "@commercetools-frontend/jest-preset-mc-app": "1.0.0-beta.11",
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "enzyme": "3.3.0",
    "eslint": "5.1.0",
    "eslint-formatter-pretty": "1.3.0",
    "husky": "0.14.3",
    "jest": "23.4.1",
    "jest-runner-eslint": "0.6.0",
    "lint-staged": "7.2.0",
    "prettier": "1.13.7"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/commercetools/merchant-center-starter-app.git"
  },
  "bugs": {
    "url": "https://github.com/commercetools/merchant-center-starter-app/issues"
  },
  "homepage": "https://github.com/commercetools/merchant-center-starter-app#readme",
  "keywords": [
    "Merchant Center",
    "starter",
    "application",
    "ui",
    "react"
  ],
  "license": "MIT"
}
